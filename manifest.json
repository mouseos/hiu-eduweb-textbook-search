{
  "manifest_version": 3,
  "name": "HIU教科書購入支援ツール",
  "version": "1.0",
  "description": "eduwebの履修確認画面でシラバス自動検索を行い、使いやすくするChrome拡張",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://eduweb.do-johodai.ac.jp/rishuu_touroku/print/student"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
		"<all_urls>"
	]
}
